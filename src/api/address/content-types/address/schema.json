{
  "kind": "collectionType",
  "collectionName": "addresses",
  "info": {
    "singularName": "address",
    "pluralName": "addresses",
    "displayName": "Address"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "address"
    },
    "full_name": {
      "type": "string"
    },
    "phone": {
      "type": "biginteger"
    },
    "address_line": {
      "type": "text"
    },
    "ward": {
      "type": "string"
    },
    "district": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "is_default": {
      "type": "boolean"
    },
    "order": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::order.order",
      "inversedBy": "address_id"
    }
  }
}
