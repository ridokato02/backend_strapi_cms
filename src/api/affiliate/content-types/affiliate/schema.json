{
  "kind": "collectionType",
  "collectionName": "affiliates",
  "info": {
    "singularName": "affiliate",
    "pluralName": "affiliates",
    "displayName": "Affiliate"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "code": {
      "type": "string"
    },
    "commission": {
      "type": "decimal"
    },
    "balance": {
      "type": "decimal"
    },
    "is_active": {
      "type": "boolean"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "affiliate_id"
    }
  }
}
