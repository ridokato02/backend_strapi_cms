{
  "kind": "collectionType",
  "collectionName": "coupons",
  "info": {
    "singularName": "coupon",
    "pluralName": "coupons",
    "displayName": "Coupon"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "code": {
      "type": "string"
    },
    "description_coupon": {
      "type": "text"
    },
    "discount_type": {
      "type": "enumeration",
      "enum": [
        "percent",
        "fixed"
      ]
    },
    "discount_value": {
      "type": "decimal"
    },
    "max_discount": {
      "type": "decimal"
    },
    "min_order": {
      "type": "decimal"
    },
    "start_date": {
      "type": "datetime"
    },
    "end_date": {
      "type": "datetime"
    },
    "usage_limit": {
      "type": "biginteger"
    },
    "per_user_limit": {
      "type": "biginteger"
    },
    "status_coupon": {
      "type": "enumeration",
      "enum": [
        "active",
        "expired",
        "disabled"
      ]
    },
    "coupon_usage": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::coupon-usage.coupon-usage",
      "mappedBy": "coupon_id"
    }
  }
}
