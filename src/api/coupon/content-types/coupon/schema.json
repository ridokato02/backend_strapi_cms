{
  "kind": "collectionType",
  "collectionName": "coupons",
  "info": {
    "singularName": "coupon",
    "pluralName": "coupons",
    "displayName": "Coupon"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "coupon_code": {
      "type": "string",
      "unique": true
    },
    "coupon_type": {
      "type": "enumeration",
      "enum": [
        "percent",
        "fixed_amount"
      ]
    },
    "coupon_value": {
      "type": "decimal"
    },
    "coupon_start_date": {
      "type": "datetime"
    },
    "coupon_end_date": {
      "type": "datetime"
    },
    "coupon_min_spend": {
      "type": "decimal"
    },
    "coupon_max_spend": {
      "type": "decimal"
    },
    "coupon_uses_per_customer": {
      "type": "biginteger"
    },
    "coupon_uses_per_coupon": {
      "type": "biginteger"
    },
    "coupon_status": {
      "type": "enumeration",
      "enum": [
        "active",
        "expired",
        "disabled"
      ]
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "coupon_id"
    }
  }
}
