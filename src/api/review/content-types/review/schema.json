{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "Review"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "review"
    },
    "product_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product",
      "inversedBy": "review"
    },
    "rating": {
      "type": "integer"
    },
    "content": {
      "type": "text"
    },
    "review_image": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::review-image.review-image",
      "mappedBy": "review"
    }
  }
}
